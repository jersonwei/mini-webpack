(function(madules){
    function require(id){
        const [fn,mapping] = modules[id]
    
        const module = {
            exports:{
            }
        }

        function localRequire(filePath){
            const id = mapping[filePath]
            return require(id)
        }
        fn(localRequire,module,module.exports)
    
        return module.exports
    }
    
    require(1)
})({
    <% data.forEach(info=>{ %>
        "<%- info["id"] %>":[function (require,module,exports){
            <%- info["code"] %>
        },<%- info["mapping"] %>]
        <% }); %>
})